{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "lint:frontend": "eslint 'frontend/**/*.{js,jsx,ts,tsx}'",
    "lint:backend": "eslint 'backend/**/*.{js,ts}'",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "sequelize": "sequelize",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "docker-compose -f docker-compose.test.yml up -d && ./wait-for-postgres.sh && npx sequelize-cli db:migrate --env test && jest --config jest.integration.config.js && docker-compose -f docker-compose.test.yml down",
    "test:all": "npm run test:unit && npm run test:integration",
    "test": "npm run test:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/mocha": "^10.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4"
  }
}
